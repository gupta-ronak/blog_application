<h2>All Your Articles</h2>
<p>Welcome <%= @account.user.name%></p>

<% if @account.user.articles.empty? %>
  <p>No Articles Written</p>
<% elsif%>
<ul>
  <% @account.user.articles.each do |article|%>
    <li>
      <a href="/article/<%= article.id%>">
      <%= article.title %>
      </a>
    </li>

  <% end%>
</ul>
<% end %>
<%= link_to "New Article", "/article/new" %>
